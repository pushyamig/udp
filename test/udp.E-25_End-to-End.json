		{
			"name": "E-25_Event-to-UDP-to-BQ",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "aeafa23b-4c74-4e41-92d9-9694498e24c3",
						"type": "text/javascript",
						"exec": [
							"eval(pm.variables.get('initRequest')); // Defined in collection pre-request script"
						]
					}
				},
				{
					"listen": "test",
					"script": {
						"id": "e68db874-f758-4e31-b73e-f733cd3b6721",
						"type": "text/javascript",
						"exec": [
							"pm.test('Response from UDP: 200 OK', function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"var resp = pm.expect(pm.response);",
							"",
							"pm.test('Response: OK to process', function () { ",
							"    resp.ok;",
							"    resp.header('content-type');",
							"    ",
							"    // FIXME: UDP should return content type \"application/json\"!  See: http://jibbering.com/blog/?p=514",
							"    pm.expect(pm.response.headers.get('content-type')).match(/^text\\/html/i);",
							"    resp.json; // Shouldn't this fail because real JSON requires content type \"application/json\"?",
							"});",
							"",
							"",
							"pm.test('UDP accepted LC event in \"Gold\" queue', function() {",
							"    var eventId = pm.globals.get('eventId');",
							"    var udpQueueName = 'STREAM-unizin-umich-umich-stream-GoldUMichLectureCapture-dev';",
							"    resp.jsonBody(eventId + '.' + udpQueueName);",
							"    var jsonData = pm.response.json();",
							"    pm.expect(parseInt(jsonData[eventId][udpQueueName], 10)).gt(40096419239401);",
							"});",
							"",
							"//header for BQ Api call",
							"var gcp_token = pm.environment.get(\"gcp_token\");",
							"var Header = require('postman-collection').Header,",
							"    headerString = 'Content-Type: application/json\\nAuthorization: Bearer '+gcp_token+'\\n';",
							"var rawHeaders = Header.parse(headerString);",
							"var jsonData = pm.response.json();",
							"var uuid_res=Object.keys(jsonData)[0];",
							"var bq = Object.keys(jsonData)[0].split(\":\")[2];",
							"// var bq = \"9eda84a3-79d0-47aa-b868-0be6dc2143f3\";",
							"let query = \"select * from learning_datasets.enriched_events where id = \"+\"'\"+bq+\"'\";",
							"        ",
							"//request variables",
							"const postReq = {",
							"  url: 'https://www.googleapis.com/bigquery/v2/projects/unizin-udp-data-umich-dev/queries',",
							"  method: 'POST',",
							"  header: rawHeaders,",
							"  body: {",
							"    mode: 'raw',",
							"    raw: JSON.stringify({",
							"    \"query\": query",
							"   })",
							"  }",
							"};",
							"",
							"console.log('Sleep for 32 sec before BQ call');",
							"delayRequestToBigQuery();",
							"",
							"// This step is importance as event will endup in Bigquery ~30sec so we query the BQ after 30 sec",
							"function delayRequestToBigQuery() {",
							"    setTimeout(function () {",
							"        pm.sendRequest(postReq, function (err, res) {",
							"            console.log(err ? err : res.json());",
							"            pm.test('Response from BQ: 200 OK', function () {",
							"               pm.expect(res).to.have.property('code', 200);",
							"               pm.expect(res).to.have.property('status', 'OK');",
							"            });",
							"            pm.test(\"Content type from BQ: JSON\", function () {",
							"                pm.expect(res.headers.get('content-type')).match(/^application\\/json/i);",
							"    ",
							"            });",
							"            pm.test(\"Event found in BQ under 30 sec\", function () {",
							"                pm.expect((res.json().rows[0].f[0].v)).to.equal(bq);",
							"            });",
							"        ",
							"        });",
							"    }, 30000);",
							"}",
							""
						]
					}
				}
			],
